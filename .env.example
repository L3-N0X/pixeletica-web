# Example environment variables for Pixeletica

# Base URL for the backend API
API_BASE_URL=http://localhost:8000

# Version of the application
APP_VERSION=1.0.0

# Node.js environment (e.g., production, development)
NODE_ENV=production

# Default locale for the application
DEFAULT_LOCALE=en

# Path for persistent data storage
DATA_VOLUME=./data

# Backend service URL (ensure proper syntax with no missing brackets)
BACKEND_SERVICE=backend:8000

# Frontend API base URL
VITE_API_BASE_URL=/api
